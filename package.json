{
  "name": "learn-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "ngodanghai9x",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "yarn start:dev",
    "postdev": "echo 'build complete'",
    "prod": "yarn start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:migrate": "ts-node ./node_modules/typeorm/cli -d ./typeOrm.config.ts migration:run",
    "db:revert": "ts-node ./node_modules/typeorm/cli -d ./typeOrm.config.ts migration:revert",
    "db:generate:postgres": "exit 1 && yarn ts-node ./node_modules/typeorm/cli migration:generate -d ./typeOrm.config.ts ./postgres-migrations/Init",
    "db:generate:mysql": "exit 1 && yarn ts-node ./node_modules/typeorm/cli migration:generate -d ./typeOrm.config.ts ./mysql-migrations/Init",
    "doc:generate": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "notUse": "",
    "typeorm": "exit 1 && ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "exit 1 && yarn typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "exit 1 && yarn typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$yarn_config_name",
    "typeorm:create-migration": "exit 1 && yarn typeorm -- migration:create ./migrations/$yarn_config_name",
    "typeorm:revert-migration": "exit 1 && yarn typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.2.1",
    "bull": "^4.10.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "graphql": "^16.6.0",
    "graphql-upload": "14",
    "helmet": "^6.0.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/koa": "^2.13.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
